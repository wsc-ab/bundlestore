{
  "name": "bundlestore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "npm run prisma:generate && npm run prisma:migrate:deploy && next build",
    "env:local": "dotenv -e .env.local",
    "env:dev": "dotenv -e .env.development.local",
    "env:prod": "dotenv -e .env.production.local",
    "dev:local": "npm run env:local -- next dev",
    "dev:dev": "npm run env:dev -- next dev",
    "dev:prod": "npm run env:prod -- next dev",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:studio:local": "npm run env:local -- npx prisma studio",
    "prisma:pull:dev": "npm run env:dev -- npx prisma db pull",
    "prisma:pull:prod": "npm run env:prod -- npx prisma db pull",
    "prisma:push:local": "npm run env:local -- npx prisma db push",
    "prisma:dev": "npm run env:local -- npx prisma migrate dev --name",
    "prisma:reset:local": "npm run env:local -- npx prisma migrate reset",
    "kill": "npx --yes kill-port 3000",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "docker:up": "docker-compose up -d",
  },
  "dependencies": {
    "@clerk/nextjs": "^4.17.1",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@prisma/client": "^4.13.0",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.3",
    "@vercel/postgres": "^0.1.3",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.0",
    "next": "13.4.0",
    "postcss": "8.4.23",
    "prisma": "^4.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.9",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9"
  }
}
